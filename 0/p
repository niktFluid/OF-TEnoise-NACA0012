FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pOut            0.714285714;

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform $pOut;

boundaryField
{
    FarField
    {
        type            inletOutlet;
        inletValue      uniform $pOut;
        value           uniform $pOut;
    }
    Inlet
    {
        type            inletOutlet;
        inletValue      uniform $pOut;
        value           uniform $pOut;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform $pOut;
        value           uniform $pOut;
    }
    Wall
    {
        type            zeroGradient;
    }
#includeEtc "caseDicts/setConstraintTypes"
}


